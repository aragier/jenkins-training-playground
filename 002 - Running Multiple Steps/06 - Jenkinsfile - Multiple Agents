pipeline {
    agent none
    stages {
    
    	stage{'mvn'}{
		agent { 
		docker { 
			image 'alpine:latest' 
		}
		
		steps{
			sh 'mvn --version'
		}
	}
	
	stage{'npm'}{
		agent { 
		docker { 
			image 'alpine:latest' 
		}
		
		steps{
			sh 'npm --version'
		}
	}
	}
    }
}
